{
    "name": "mayanksdudakiya/state-machine-for-orders",
    "description": "To ensure correct tracking and updating of each status of each each order, it is necessary to implement the State design pattern.",
    "keywords": [
        "mayanksdudakiya",
        "state-machine-for-orders"
    ],
    "homepage": "https://github.com/mayanksdudakiya/state-machine-for-orders",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mayank Dudakiya",
            "email": "mayanksdudakiya@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mayanksdudakiya\\StateMachineForOrders\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mayanksdudakiya\\StateMachineForOrders\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mayanksdudakiya\\StateMachineForOrders\\StateMachineForOrdersServiceProvider"
            ],
            "aliases": {
                "StateMachineForOrders": "Mayanksdudakiya\\StateMachineForOrders\\StateMachineForOrdersFacade"
            }
        }
    }
}
